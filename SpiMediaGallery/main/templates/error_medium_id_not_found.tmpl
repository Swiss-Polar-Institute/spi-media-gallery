{% extends 'base.tmpl' %}
{% block title %}SPI Media Gallery - Medium ID not found{% endblock %}

{% block body_contents %}
    <p>
        <div class="container">
            {% include "logo.tmpl" %}
            <p></p>
            Photo ID not found: {{ medium_id_not_found }}

            Try a different one:
            <h2>Search using Medium ID</h2>
            <form action="/search/id/" method="POST">
                {% csrf_token %}

                <table>
                    {{ form_search_medium_id }}
                </table>
                <input type="submit" value="Search">
            </form>

        Or go to the <a href="/">homepage</a>.
        </div>
    </p>
{% endblock %}